@model Stokvel_Groups_Home.Models.Deposit

@{
    ViewData["Title"] = "Create";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Create</h1>



<h4>Deposit</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="DepositAmount" class="control-label"></label>
                <input asp-for="DepositAmount" id="depositamount" class="form-control" />
                <span asp-validation-for="DepositAmount" class="text-danger"></span>
            </div>
            
            <div class="form-group">
                <label asp-for="DepositReference" class="control-label"></label>
                <select asp-for="DepositReference" class="form-control" id="accounttype" oninput="success()" name="demo">
                    <option>Select Gender</option>
                    <option>Summer</option>
                    <option>Autumn</option>
                    <option>Winter</option>
                    <option>Spring</option>
                </select>
                <span asp-validation-for="DepositReference" class="text-danger"></span>
            </div>
            <div class="form-group">
                <button type="submit" id="buttondepo" disabled class="btn btn-primary"> Create</button>
            </div>

        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

