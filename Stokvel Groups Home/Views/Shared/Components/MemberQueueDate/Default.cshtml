@model IEnumerable<DisplayMemberTurn>

<div class="col-md-6">
    <div class="dash_blog">
        <div class="dash_blog_inner">
            <div class="dash_head">
                <h3><span><i class="fa fa-users"></i> Stokvel Queue</span></h3>
            </div>
            <div class="list_cont">
                <p>Member Queue Date</p>
            </div>
            <div class="msg_list_main">
                <ul class="msg_list">

                    @foreach (var member in Model)
                    {
                                <li>
                            <span>
                                @if ( @ViewBag.image != @ViewBag.MemberPhotoPath)
                                {
                                    <img src="@Url.Content("~/images/MemberProfile/" + @member.AccountUser.MemberFileName)" height="60px" width="60px" />
                                }
                                else
                                {
                                    <img src="@Url.Content("~/images/Profile/" + @member.AccountUser.MemberFileName)" height="60px" width="60px" />
                                }
                            </span>
                                <span>
                                <span class="name_user">@member.AccountUser.FirstName @member.AccountUser.LastName</span>
                                <span class="msg_user">Position Number: @member.Account.AccountQueue </span>
                                <span class="time_ago"> @member.Account.AccountQueueStart.ToString("yyyy-MM-dd") </span>
                                    </span>
                                </li>
                    }
                       
                </ul>
            </div>
            <div class="read_more">
            </div>
        </div>
    </div>
</div>
