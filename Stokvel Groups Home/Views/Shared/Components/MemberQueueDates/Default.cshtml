@model IEnumerable<DisplayMemberTurn>

<div class="col-md-6">
    <div class="dash_blog">
        <div class="dash_blog_inner">
            <div class="dash_head">
                <h3><span><i class="fa fa-users"></i> Stokvel Queue</span></h3>
            </div>
            <div class="list_cont">
                <p>Member Queue Date</p>
            </div>
            <div class="msg_list_main">
                <ul class="msg_list">

                    @foreach (var member2 in Model)
                    {
                                <li>
                                    <span><img src="https://mdbcdn.b-cdn.net/img/Photos/new-templates/bootstrap-chat/ava2-bg.webp" class="img-responsive" alt="#" /></span>
                                    <span>
                                <span class="name_user">@member2.AccountUser.FirstName @member2.AccountUser.LastName</span>
                                <span class="msg_user">Position Number: @member2.Account.AccountQueue </span>
                                <span class="time_ago"> @member2.Account.AccountQueueStart.ToString("yyyy-MM-dd") </span>
                                    </span>
                                </li>
                    }
                       
                </ul>
            </div>
            <div class="read_more">
            </div>
        </div>
    </div>
</div>
